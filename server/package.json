  "main": "index.js",
  "scripts": {
    "deploy": "../node_modules/.bin/concurrently \\\"npm run build\\\" \\\"npm run buildClient\\\"",
    "start": "nodemon server.js",
    "build": "../node_modules/.bin/tsc -p .",
    "build:live": "../node_modules/.bin/nodemon --watch \\\"server/**/*.ts\\\" --exec \\\"ts-node\\\" server/index.ts",
    "client": "npm run start --prefix ../client",
    "buildClient": "npm run build --prefix ../client",
    "dev": "concurrently –kill-others-on-fail \"npm run start\" \"npm run client\"",
    "debug": "concurrently –kill-others-on-fail \"nodemon --inspect server.js\" \"npm run client\""
  },
